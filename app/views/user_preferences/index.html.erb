<h1>Listing user_preferences</h1>

<table>
  <tr>
    <th>User</th>
    <th>Custom</th>
    <th>Language</th>
    <th>Active</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_preferences.each do |user_preference| %>
  <tr>
    <td><%= user_preference.user_id %></td>
    <td><%= user_preference.custom_id %></td>
    <td><%= user_preference.language %></td>
    <td><%= user_preference.active %></td>
    <td><%= link_to 'Show', user_preference %></td>
    <td><%= link_to 'Edit', edit_user_preference_path(user_preference) %></td>
    <td><%= link_to 'Destroy', user_preference, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

