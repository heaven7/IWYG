<h1>Listing user_preferences</h1>

<table>
  <tr>
    <th>User</th>
    <th>Custom</th>
    <th>Language</th>
    <th>Active</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_preferences.each do |user_preference| %>
  <tr>
    <td><%= user_preferences.user_id %></td>
    <td><%= user_preferences.custom_id %></td>
    <td><%= user_preferences.language %></td>
    <td><%= user_preferences.active %></td>
    <td><%= link_to 'Show', user_preferences %></td>
    <td><%= link_to 'Edit', edit_user_preference_path(user_preferences) %></td>
    <td><%= link_to 'Destroy', user_preferences, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

