<% if images %>
  <div class="list images">  
    <% images.each do |i| %>
      <% if !i.image.url.include? "missing" %>
        <span class="image">
          <%= link_to image_tag(i.image.url(:small), class: "ui-corner-all"), i.image.url(:big), class: "colorbox" %>
        </span>
     <% end %>
    <% end %>
  </div>
<% end %>
